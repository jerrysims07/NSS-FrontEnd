%head
  %meta{charset: "utf-8"}
  %meta{name: "viewport", content: "width=device-width"}
  %title Foundation 4
  %link{rel: "stylesheet", href: "css/normalize.css"}
  %link{rel: "stylesheet", href: "css/foundation.css"}
  %link{rel: "stylesheet", href: "css/app.css"}
  %script{src: "js/vendor/custom.modernizr.js"}
%body
  .row
    .small-12.columns
      %h1 Flickr Project
  .row
    .small-2.columns
      %input#query.radius{type: 'text', autofocus: true}
    .small-1.columns
      %input#search.button.radius.small{type: 'button', value: 'search'}
    .small-1.columns
      %input#save.button.radius.small.success{type: 'button', value: 'save'}
    .small-1.columns
      %input#remove.button.radius.small.alert{type: 'button', value: 'clear'}
    .small-1.columns
      %input#deleteSpecific.button.radius.small.alert{type: 'button', value: 'remove selected'}
    .small-6.columns
  .row
    .small-8.columns
      %h4.subheader Search Results
      #photos
    .small-4.columns
      %h4.subheader Saved Photos
      #savedPhotos



  %script{src: 'js/vendor/jquery-1.10.2.js'}
  %script{src: 'js/vendor/foundation.min.js'}
  %script{src: 'js/vendor/underscore.js'}
  %script{src: 'js/app/app.js'}
