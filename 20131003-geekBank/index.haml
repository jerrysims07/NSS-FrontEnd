!!! 5
%html{lang: 'en'}
  %head
    %meta{charset: 'utf-8'}
    %title Template
    %link{rel: 'stylesheet', href: 'css/normalize.css'}
    %link{rel: 'stylesheet', href: 'css/qunit-theme-gabe.css'}
    %link{rel: 'stylesheet', href: 'css/style.css'}
  %body
    #wrap
      %h1 The First Bank of Mandalore
      %img#logo
      #logoInputArea
        %input#imageSourceName{type: 'text'}
        %input#loadImageButton{type: 'button', value: 'Set Logo'}
      .clear
      %section#activitySection
        %balanceDisplayArea
          %h2 Balance
          %span $
          %input#balanceDisplay{type: 'text', readonly: true}
        #startingBalanceArea
          %input#balanceEntry{type: 'text'}
          %input#setBalanceButton{type: 'button', value: 'Set Balance'}
        .clear
        %input#transactionAmount{type: 'text'}
        %input#depositButton{type: 'button', value: 'Deposit'}
        %input#withdrawButton{type: 'button', value: 'Withdraw'}
      %section#reportingSection
        %h2
          Transaction List
        .clear
        %ul#depositList Deposits
        %ul#withdrawList Withdraws


    %script{src: 'js/jquery-1.10.2.min.js'}
    %script{src: 'js/underscore-min.js'}
    %script{src: 'js/qunit-1.12.0.js'}
    %script{src: 'js/main.js'}
    %script{src: 'js/testing.js'}